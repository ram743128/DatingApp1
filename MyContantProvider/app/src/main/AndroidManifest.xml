<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ram.mycontantprovider">
    <permission android:name="com.ram.mycontantprovider.READ"/>
    <permission android:name="com.ram.mycontantprovider.WRITE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".MyContentProvider"
            android:authorities="com.ram.mycontantprovider"
            android:readPermission="com.ram.mycontantprovider.READ"
            android:writePermission="com.ram.mycontantprovider.WRITE"
            android:enabled="true"
            android:exported="true"></provider>
    </application>

</manifest>